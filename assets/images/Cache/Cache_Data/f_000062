<script>
    // Get the container or document
    const container = document; // Change this to your specific container if needed

    // Add a click event listener to the container or document
    container.addEventListener('click', function(event) {
        // Check if the clicked element is not within '.new-language' or '.display-lang'
        if (!event.target.closest('.new-language') && !event.target.closest('.display-lang')) {
            // Hide '.display-lang'
            document.querySelectorAll('.display-lang').forEach(function(displayLangElement) {
                displayLangElement.style.display = 'none';
            });

            // Reset rotation for '.fa-chevron-down'
            document.querySelectorAll('.fa-chevron-down').forEach(function(element) {
                element.style.transform = 'rotate(0deg)';
            });
        }
    });

    // Add a click event listener to '.new-language'
    document.querySelectorAll('.new-language').forEach(function(element) {
        element.addEventListener('click', function() {
            // Toggle the display of '.display-lang'
            document.querySelectorAll('.display-lang').forEach(function(displayLangElement) {
                displayLangElement.style.display = (displayLangElement.style.display === 'block') ? 'none' : 'block';
            });

            // Toggle the rotation for '.fa-chevron-down'
            document.querySelectorAll('.fa-chevron-down').forEach(function(element) {
                const currentRotation = parseInt(element.style.transform.replace('rotate(', '').replace('deg)', '')) || 0;
                const newRotation = currentRotation === 0 ? 180 : 0;
                element.style.transform = 'rotate(' + newRotation + 'deg)';
            });

            // Prevent the click event from propagating to the container/document
            event.stopPropagation();
        });
    });
</script>
